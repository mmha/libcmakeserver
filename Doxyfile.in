PROJECT_NAME = @PROJECT_NAME@
PROJECT_NUMBER = @PROJECT_VERSION@
INPUT = @PROJECT_SOURCE_DIR@/libcmakeserver
INPUT += @PROJECT_SOURCE_DIR@/README.md
USE_MDFILE_AS_MAINPAGE = README.md
RECURSIVE = YES

FULL_PATH_NAMES = YES
STRIP_FROM_PATH = @PROJECT_SOURCE_DIR@/libcmakeserver

HAVE_DOT = @USE_DOT@
DOT_IMAGE_FORMAT=svg
EXTRACT_ALL = YES
EXTRACT_PRIVATE = YES
EXTRACT_PACKAGE = YES
EXTRACT_STATIC = YES
CLASS_GRAPH = YES
COLLABORATION_GRAPH = YES
TEMPLATE_RELATIONS = YES
HIDE_SCOPE_NAMES = YES
MAX_INITIALIZER_LINES = 0
#UML_LOOK = YES

ENABLE_PREPROCESSING   = YES
MACRO_EXPANSION        = YES
EXPAND_ONLY_PREDEF     = NO
JAVADOC_AUTOBRIEF      = YES

GENERATE_LATEX         = NO
GENERATE_HTML          = NO
GENERATE_XML           = YES
XML_OUTPUT             = .

EXCLUDE_PATTERNS = @PROJECT_BINARY_DIR@
EXCLUDE_PATTERNS += @PROJECT_SOURCE_DIR@/.git

#CLANG_ASSISTED_PARSING = YES
#CLANG_COMPILATION_DATABASE_PATH = @PROJECT_BINARY_DIR@

PREDEFINED = DONT_DOCUMENT
